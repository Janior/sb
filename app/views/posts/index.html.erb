<div class="container">
	<div class="row">
		<div class="col-sm-12">
			<h1>Posts</h1>

			<table class="table">
				<tr>
					<th>Id</th>
					<th>Titulo</th>
					<th>Cuerpo</th>
				</tr>
				<% @posts.each do |post| %>
				<tr>
					<td> <%= post.id %> </td>
					<td> <%= post.titulo %> </td>		
					<td> <%= post.cuerpo %> </td>
				</tr>
				<% end %>
			</table>			
		</div>
	</div>
</div>


